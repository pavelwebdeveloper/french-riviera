<p id="infomessage"><%= infomessage %></p>

		<form>
             <fieldset>
				<div class="form-group">
				<label for="userEmail">E-mail*:</label>
				<input class="form-control form-control-custom" type="email" id="userEmail" name="userEmail" placeholder="someone@gmail.com" pattern="[a-z0-9\._%+-]+@[a-z0-9.]+\.[a-z]{2,}$" required>
				</div>
				<div class="form-group">
				<label for="userPassword">Password*:</label>
				<input class="form-control form-control-custom" type="password" name="userPassword" id="userPassword" pattern="(?=^.{8,}$)(?=.*\d)(?=.*\W+)(?![.\n])(?=.*[A-Z])(?=.*[a-z]).*$" required>
				</div>
			</fieldset>
		
		<button class="btn btn-success login-form-button" type="submit" onclick="logIn()">Log in</button>
		<p id="login">Not registered yet?</p>
		<a class="btn btn-success login-form-button" id="aregister" onclick="getSignUpForm()" title="a link to a sign_up page">Sign Up</a>
		
		</form>